<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#19000000" android:endColor="#19000000" android:centerX="0.75" android:centerColor="#19000000" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#19000000" android:endColor="#19000000" android:centerX="0.75" android:centerColor="#19000000" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/color_height" android:endColor="@color/color_height" android:centerX="0.75" android:centerColor="@color/color_height" />
            </shape>
        </clip>
    </item>
</layer-list>